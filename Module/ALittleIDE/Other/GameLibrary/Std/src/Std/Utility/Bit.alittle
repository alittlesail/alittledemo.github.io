
namespace ALittle;

public static BitAnd(int x, int y) : int
{
    [Language(Lua)]
    return lua.bit.band(x, y);

    [Language(JavaScript)]
    return javascript.bit.band(x, y);
}

public static BitOr(int x, int y) : int
{
    [Language(Lua)]
    return lua.bit.bor(x, y);

    [Language(JavaScript)]
    return javascript.bit.bor(x, y);
}